{
  "name": "testnext",
  "version": "0.0.2",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "pal g -m && yarn generate:prisma1 && yarn generate:prisma2 && yarn generate:nexus && yarn codegen",
    "generate:prisma1": "prisma generate --schema=prisma1/schema.prisma",
    "generate:prisma2": "prisma generate --schema=prisma2/schema.prisma",
    "generate:nexus": "yarn ts-node --transpile-only src/server/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "db-dev": "prisma migrate dev --schema=prisma1/schema.prisma && prisma migrate dev --schema=prisma2/schema.prisma",
    "db-deploy": "prisma migrate deploy --schema=prisma1/schema.prisma && prisma migrate deploy --schema=prisma2/schema.prisma",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "@chakra-ui/react": "1.8.5",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@heroicons/react": "1.0.5",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@paljs/admin": "5.0.0",
    "@paljs/nexus": "5.0.0",
    "@prisma/client": "3.10.0",
    "apollo-server-micro": "3.6.3",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.2",
    "framer-motion": "6.2.8",
    "graphql": "15.7.2",
    "graphql-tag": "2.12.6",
    "jsonwebtoken": "8.5.1",
    "micro": "9.3.4",
    "next": "12.1.0",
    "nexus": "1.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-node": "10.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.8",
    "@paljs/cli": "5.0.0",
    "@types/bcryptjs": "2.4.2",
