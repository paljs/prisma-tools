{
  "name": "testnext",
  "version": "0.0.2",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "pal g -m && yarn generate:prisma1 && yarn generate:prisma2 && yarn generate:nexus && yarn codegen",
    "generate:prisma1": "prisma generate --schema=prisma1/schema.prisma",
    "generate:prisma2": "prisma generate --schema=prisma2/schema.prisma",
    "generate:nexus": "yarn ts-node --transpile-only src/server/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "db-dev": "prisma migrate dev --schema=prisma1/schema.prisma && prisma migrate dev --schema=prisma2/schema.prisma",
    "db-deploy": "prisma migrate deploy --schema=prisma1/schema.prisma && prisma migrate deploy --schema=prisma2/schema.prisma",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "dependencies": {
    "@apollo/client": "3.4.10",
    "@chakra-ui/react": "1.6.7",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@heroicons/react": "1.0.4",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@paljs/admin": "4.0.0",
    "@paljs/nexus": "4.0.8",
    "@prisma/client": "3.0.1",
    "apollo-server-micro": "3.3.0",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.1",
    "framer-motion": "4.1.17",
    "graphql": "15.5.3",
    "graphql-tag": "2.12.5",
    "jsonwebtoken": "8.5.1",
    "micro": "9.3.4",
    "next": "11.1.2",
    "nexus": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-node": "10.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.1.3",
    "@graphql-codegen/typescript-react-apollo": "3.1.3",
    "@paljs/cli": "4.0.8",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.1",
    "@types/graphql": "14.5.0",
