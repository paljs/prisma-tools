{
  "name": "apollo-sdl-first-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "prisma and apollo server  example to create CUED system by one command",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "pal g && prisma generate",
    "db-dev": "prisma migrate dev",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server"
  },
  "dependencies": {
    "@graphql-tools/merge": "8.x",
    "@graphql-tools/schema": "9.x",
    "@paljs/plugins": "5.3.3",
    "@prisma/client": "4.x",
    "apollo-server": "3.x",
    "graphql": "16.x",
    "graphql-middleware": "6.x"
  },
  "devDependencies": {
    "prisma": "4.x",
    "@types/node": "18.x",
    "@paljs/cli": "5.3.3",
    "prettier": "2.x",
    "ts-node": "10.x",
    "ts-node-dev": "2.x",
    "typescript": "4.x"
  }
}
