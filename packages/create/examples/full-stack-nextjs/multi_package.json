{
  "name": "testnext",
  "version": "0.0.2",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "pal g -m && yarn generate:prisma1 && yarn generate:prisma2 && yarn generate:nexus && yarn codegen",
    "generate:prisma1": "prisma generate --schema=prisma1/schema.prisma",
    "generate:prisma2": "prisma generate --schema=prisma2/schema.prisma",
    "generate:nexus": "yarn ts-node --transpile-only src/server/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "db-dev": "prisma migrate dev --schema=prisma1/schema.prisma && prisma migrate dev --schema=prisma2/schema.prisma",
    "db-deploy": "prisma migrate deploy --schema=prisma1/schema.prisma && prisma migrate deploy --schema=prisma2/schema.prisma",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "dependencies": {
    "@apollo/client": "3.x",
    "@chakra-ui/react": "1.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@heroicons/react": "1.x",
    "@material-ui/core": "4.x",
    "@material-ui/icons": "4.x",
    "@paljs/admin": "4.0.0",
    "@paljs/nexus": "4.0.0",
    "@prisma/client": "3.x",
    "apollo-server-micro": "3.x",
    "bcryptjs": "2.x",
    "cookie": "0.x",
    "framer-motion": "4.x",
    "graphql": "15.x",
    "graphql-tag": "2.x",
    "jsonwebtoken": "8.x",
    "micro": "9.x",
    "next": "11.x",
    "nexus": "1.x",
    "react": "17.x",
    "react-dom": "17.x",
    "ts-node": "10.x"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.x",
    "@graphql-codegen/typescript": "2.x",
    "@graphql-codegen/typescript-operations": "2.x",
    "@graphql-codegen/typescript-react-apollo": "3.x",
    "@paljs/cli": "4.0.4",
    "@types/bcryptjs": "2.x",
    "@types/cookie": "0.x",
    "@types/graphql": "14.x",
    "@types/jsonwebtoken": "8.x",
